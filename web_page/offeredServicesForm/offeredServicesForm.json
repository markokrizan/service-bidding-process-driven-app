    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "console.log($data);",
        "",
        "if (!$data.offeredServices_serviceCategories || !$data.offeredServices_serviceCategories.length) {",
        "    return;",
        "}",
        "",
        "var output = {",
        "\t//map offeredServices variable to expected task contract input",
        "\tofferedServicesInput: {",
        "\t\tuserId: $data.loggedInUser.user_id,",
        "\t\tserviceCategories: $data.offeredServices_serviceCategories.map(it => {",
        "\t\t    console.log({it: it});",
        "\t\t    return {",
        "\t\t      persistenceId_string: it",
        "\t\t    };",
        "\t\t})",
        "\t}",
        "}",
        "",
        "console.log({output: output});",
        "",
        "return output;"
      ],
      "exposed" : false
    },
    "context" : {
